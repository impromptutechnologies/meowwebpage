(function() {
  var start = new Date;
  start.setUTCHours(14, 30, 0);
  var end = new Date;
  end.setUTCHours(21, 30, 0); // 11pm
  function pad(num) {
    return ("0" + parseInt(num)).substr(-2);
  }
  function tick() {
    var now = new Date;
    console.log(now > start, now < end)
    if (now > start && now < end) { // too late, go to tomorrow
      //start.setDate(start.getDate() + 1);
      document.getElementById('time').innerHTML =
      "Market Open. Commands placed will be for next trading day"
    } if(now > end){
      var remain = ((start - now) / 1000);
    var hh = pad((remain / 60 / 60) % 60);
    var mm = pad((remain / 60) % 60);
    var ss = pad(remain % 60);
    document.getElementById('time').innerHTML =
      "Time to place command: " + hh + ":" + mm + ":" + ss;
      start.setDate(start.getDate() + 1)
      document.getElementById('time').innerHTML = hh + ":" + mm + ":" + ss;
    }
    
    setTimeout(tick, 1000);
  }
  document.addEventListener('DOMContentLoaded', tick);
})();

</script>

