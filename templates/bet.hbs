            <script type="text/javascript">
      
            function changeTimezone() {
                    
                var values = document.getElementsByClassName("date");
                
                Object.keys(values).forEach((element) => {
                    var stillUtc = moment.utc(values[element].innerHTML, 'MM-DD hh:mm').toDate();
                    var time2 = moment(stillUtc).local().format('MM-DD hh:mm A');
                    values[element].innerHTML = time2;

                })
          
                    
            }
        </script>        
            
           
